
.all {
	margin-top: 20rpx;
}
.all .bar {
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-orient: horizontal;
	-webkit-box-direction: normal;
	-webkit-flex-direction: row;
	        flex-direction: row;
	-webkit-box-pack: justify;
	-webkit-justify-content: space-between;
	        justify-content: space-between;
	margin: 30rpx 20rpx;
	padding: 10rpx;
}
.bar .barbtn {
	height: 30px;
	line-height: 30px;
	font-size: 12px;
}
.all .week {
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-orient: horizontal;
	-webkit-box-direction: normal;
	-webkit-flex-direction: row;
	        flex-direction: row;
	-webkit-box-pack: justify;
	-webkit-justify-content: space-between;
	        justify-content: space-between;
	padding: 20rpx;
	padding-left: 40rpx;
	padding-right: 40rpx;
	margin: 20rpx;
	-webkit-border-radius: 10px;
	        border-radius: 10px;
	background-color: #fff;
}
.myDateTable {
	margin: 2.5vw;
	padding: 2vw;
	-webkit-border-radius: 10px;
	        border-radius: 10px;
	background: -webkit-gradient(linear, left top, left bottom, from(#74AADA), to(#94db98));
	background: -webkit-linear-gradient(#74AADA, #94db98);
	background: linear-gradient(#74AADA, #94db98);
}
.myDateTable .dateCell {
	width: 11vw;
	padding: 1vw;
	display: inline-block;
	text-align: center;
	font-size: 16px;
}
.dateCell .cell {
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-border-radius: 50%;
	        border-radius: 50%;
	height: 11vw;
	-webkit-box-pack: center;
	-webkit-justify-content: center;
	        justify-content: center;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
}
.whiteColor {
	color: #fff;
}
.greenColor {
	color: #01b90b;
	font-weight: bold;
}
.bgWhite {
	background-color: #fff;
}
.bgGray {
	background-color: rgba(255, 255, 255, 0.42);
}
.bgBlue {
	font-size: 14px;
	background-color: #4b95e6;
}
.redColor {
	color: #ff0000;
}
.TipArea {
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-pack: center;
	-webkit-justify-content: center;
	        justify-content: center;
	word-break: break-all;
	word-wrap: break-word;
	font-size: 14px;
	padding: 10px;
}
.impTip {
	display: inline-block;
	color: #ff0000;
	font-size: 18px;
	font-weight: bold;
	margin: 0 6rpx;
}
.location-btn {
	margin: 20px auto;
	width: 80px;
	height: 80px;
	line-height: 80px;
	-webkit-border-radius: 50%;
	        border-radius: 50%;
	border: none;
}
button::after {
	display: none;
}

